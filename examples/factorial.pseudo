# Пример 1: Вычисление факториала числа
# Демонстрирует цикл for и арифметические операции

number = 5;
factorial = 1;

print("Calculating factorial of " + number);

for i in range(1, number + 1) {
    factorial = factorial * i;
    print("Step " + i + ": factorial = " + factorial);
}

print("Result: " + number + "! = " + factorial);
